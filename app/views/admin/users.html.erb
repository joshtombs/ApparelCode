<h1>Users</h1>
<table>
  <tr>
    <th width="30%">Username</th>
    <th width="20%">Email</th>
    <th width="20%">Country</th>
    <th width="20%">Last Signed In On</th>
  </tr>
  <% for user in @users %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td><%= user.country %></td>
      <td><%= user.last_signed_in_on %></td>
      <td><%= link_to 'Delete', admin_path(user), :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>
